VERSION v2.0
CLEARLOG
COPY $count 0
@loop
    CALL recursive_fibb $count -> $result
    PRINT "F(%d) = %d" $count $result
    INC $count $count
    SUB $test $count 8
    GOTOIF @loop $test
END

@recursive_fibb(N) 
    SUB $Nm1 $N 1
    SUB $Nm2 $N 2
    GOTOIFN @case0 $N
    GOTOIFN @case1 $Nm1

    CALL recursive_fibb $Nm1 -> $F_Nm1
    CALL recursive_fibb $Nm2 -> $F_Nm2
    ADD $F_N $F_Nm1 $F_Nm2
    RETURN $F_N
    
    @case0
    RETURN 0
    @case1
    RETURN 1