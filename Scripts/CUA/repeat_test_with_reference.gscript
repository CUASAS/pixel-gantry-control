SETLOG "repeat_test_with_reference_20.log"
CLEARLOG
SETVAC module_chuck4_slot3 1

COPY $fid_pos {686.776,275.695,62.54398}
COPY $count 200

@LOOP MOVETO {600,150,62.54398} 20
      MOVETO {700,250,62.54398} 20
      MOVETO $fid_pos 20
      WAIT 1000
      FINDFID $pos FPIX_HDI
      PRINT "FIDPOS %d %v" $count $pos
      DEC $count $count
      GOTOIF @LOOP $count
PRINT "Done"


