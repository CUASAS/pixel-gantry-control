VERSION v2.1
CLEARLOG
COPY $count 0
@loop
    CALL recursive_fibb $count -> $result
    PRINT "F(%d) = %d" $count $result
    INC $count $count
    GOTOIF @loop `$count<i=10`
END

@recursive_fibb(N)
    GOTOIF case0 `$N=i=0`  # leading @ is optional for label args
    GOTOIF @case1 `$N=i=1`

    CALL recursive_fibb `$N-1` -> $F_Nm1
    CALL recursive_fibb `$N-2` -> $F_Nm2
    ADD $F_N $F_Nm1 $F_Nm2
    RETURN $F_N
    
    @case0
    RETURN 0
    @case1
    RETURN 1